<Window x:Class="HellMessenger.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HellMessenger"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DockPanel Name="mainPanel">
            <DockPanel DockPanel.Dock="Top" Name="settingsPanel"></DockPanel>
            <DockPanel DockPanel.Dock="Bottom" Name="sendPanel"></DockPanel>
            <DockPanel DockPanel.Dock="Left" Name="usersPanel"></DockPanel>
            <DockPanel DockPanel.Dock="Right" Name="extraPanel"></DockPanel>
            <DockPanel Name="chatPanel"></DockPanel>
        </DockPanel>
    </Grid>
</Window>
